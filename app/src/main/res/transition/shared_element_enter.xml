<?xml version="1.0" encoding="utf-8"?>
<transitionSet xmlns:android="http://schemas.android.com/apk/res/android"
    android:duration="@integer/element_animation_duration">
    <changeBounds />
    <changeTransform />
    <changeImageTransform />
    <transitionSet android:transitionOrdering="sequential">
        <transition
            class="com.wolffincdevelopment.hiit_it.util.RevealTransition"
            android:transitionVisibilityMode="mode_out" />
        <transition
            class="com.wolffincdevelopment.hiit_it.util.RevealTransition"
            android:transitionVisibilityMode="mode_in" />
        <targets>
            <target android:targetId="@id/content" />
            <target android:targetName="snapshot" />
        </targets>
    </transitionSet>
</transitionSet>