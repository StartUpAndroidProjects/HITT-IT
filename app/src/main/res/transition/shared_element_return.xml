<?xml version="1.0" encoding="utf-8"?>
<transitionSet xmlns:android="http://schemas.android.com/apk/res/android"
    android:duration="@integer/element_animation_duration" android:transitionOrdering="sequential">
    <transition class="com.wolffincdevelopment.hiit_it.util.RevealTransition"
        android:transitionVisibilityMode="mode_out">
        <targets>
            <target android:targetId="@id/content" />
        </targets>
    </transition>
    <transitionSet>
        <changeBounds />
        <changeTransform />
        <changeImageTransform />
        <transition class="com.wolffincdevelopment.hiit_it.util.RevealTransition"
            android:transitionVisibilityMode="mode_in">
        </transition>
    </transitionSet>
</transitionSet>